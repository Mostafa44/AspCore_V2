@page
@model OdeToFood.Pages.Restaurants.ListModel
@{
}
<h1>Restaurants</h1>

<table class="table">
    @foreach (var item in Model.restaurants)
    {
        <tr>

            <td>@item.Name</td>
            <td>@item.Location</td>
            <td>@item.Cuisine</td>
        </tr>
    }
</table>

<div>@Model.Message</div>